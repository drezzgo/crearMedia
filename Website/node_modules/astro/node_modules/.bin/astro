#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/agong/Documents/Programming/CrearMedia/Website/node_modules/.pnpm/astro@5.15.1_@types+node@24_c697064d5d2480de6acfec2405c963d8/node_modules/astro/node_modules:/proc/cygdrive/c/Users/agong/Documents/Programming/CrearMedia/Website/node_modules/.pnpm/astro@5.15.1_@types+node@24_c697064d5d2480de6acfec2405c963d8/node_modules:/proc/cygdrive/c/Users/agong/Documents/Programming/CrearMedia/Website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/agong/Documents/Programming/CrearMedia/Website/node_modules/.pnpm/astro@5.15.1_@types+node@24_c697064d5d2480de6acfec2405c963d8/node_modules/astro/node_modules:/proc/cygdrive/c/Users/agong/Documents/Programming/CrearMedia/Website/node_modules/.pnpm/astro@5.15.1_@types+node@24_c697064d5d2480de6acfec2405c963d8/node_modules:/proc/cygdrive/c/Users/agong/Documents/Programming/CrearMedia/Website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../astro.js" "$@"
else
  exec node  "$basedir/../../astro.js" "$@"
fi
